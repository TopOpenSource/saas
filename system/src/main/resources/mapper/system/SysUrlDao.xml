<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdstc.system.dao.SysUrlDao">
	
	<resultMap id="SysUrlMap" type="com.sdstc.system.model.SysUrl">
	    <id column="id" property="id" />
	    <result column="gmt_create" property="gmtCreate" />
	    <result column="gmt_modified" property="gmtModified" />
	    <result column="create_account" property="createAccount" />
	    <result column="modified_account" property="modifiedAccount" />
	    <result column="is_delete" property="isDelete" />
	    <result column="name" property="name" />
	    <result column="url" property="url" />
	    <result column="p_id" property="pId" />
	    <result column="index" property="index" />
	    <result column="path" property="path" />
	</resultMap>
	
    <sql id="SysUrlCols">
      id,gmt_create,gmt_modified,create_account,modified_account,is_delete,name,url,p_id,index,path
    </sql>
    
    <sql id="SysUrlProps">
      #{id},#{gmtCreate},#{gmtModified},#{createAccount},#{modifiedAccount},#{isDelete},#{name},#{url},#{pId},#{index},#{path}
    </sql>
    
    <sql id="SysUrlWhere">
      
    </sql>
    
    <sql id="SysUrlDtoWhere">
      
    </sql>
	
	<select id="selectByDto"  parameterType="com.sdstc.system.model.SysUrl" resultMap="SysUrlMap">
		select
			<include refid="SysUrlCols"></include>
		from sys_url
		<where>
		    <include refid="SysUrlWhere"></include>
		</where>
	</select>
	
	<select id="selectCountByDto"  parameterType="com.sdstc.system.model.SysUrl" resultType="Integer">
		select
			count(*)
		from sys_url
		<where>
		    <include refid="SysUrlWhere"></include>
		</where>
	</select>
	
	<select id="selectPageByDto" resultMap="SysUrlMap">
		select
			<include refid="SysUrlCols"></include>
		from sys_url
		<where>
		    <include refid="SysUrlDtoWhere"></include>
		</where>
		limit #{pageDto.start},#{pageDto.pageSize}
	</select>
</mapper>